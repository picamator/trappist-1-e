  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0"
  }
