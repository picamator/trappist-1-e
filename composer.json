{
  "name": "picamator/trappist-1-e",
  "description": "Challenge project with two solutions.",
  "keywords": ["Challenge"],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergii Pryz"
    }
  ],
  "support": {
